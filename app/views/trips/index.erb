<h1>Trips</h1>

<div class="trips">
  <a href="/trips/new"> Add New Trip </a>
  <br>
  <br>
  <% @trips.each do |trip| %>
    Trip Id:<a href="/trips/<%= trip.id %>"> <%= trip.id %></a>
    <p>Duration: <%= trip.duration %></p>
    <p>Start Date: <%= trip.start_date %></p>
    <p>Start Station: <%= trip.start_station_name %></p>
    <p>End Date:<%= trip.end_date %></p>
    <p>End Station:<%= trip.end_station_name %></p>
    <p>Bike ID:<%= trip.bike_id %></p>
    <p>Subscription Type: <%= trip.subscription %></p>
    <p>Zip Code: <%= trip.zip_code %></p>
  <% end %>

</div>
